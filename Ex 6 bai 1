package baitaappp;

import java.util.Scanner;

/**
 * Chương trình liệt kê N giá trị đầu tiên của bộ ba (a, b, c) sao cho các điều kiện được đề cập.
 */
public class baif1 {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Nhập giá trị N từ người dùng
        System.out.print("Nhập giá trị N (0 ≤ N ≤ 505): ");
        int N = scanner.nextInt();

        // Kiểm tra điều kiện cho các tam giác và liệt kê N giá trị thỏa mãn
        int count = 0; // Đếm số tam giác đã tìm được
        int a, b, c;

        outerLoop:
        for (a = 1; a <= 10; a++) {
            for (b = 1; b <= 10; b++) {
                for (c = 1; c <= 10; c++) {
                    // Kiểm tra điều kiện tam giác
                    if ((a == 5 && c == 7) || (b == 4 && c == 4)) {
                        continue; // Bỏ qua các tam giác không thoả mãn điều kiện
                    }

                    // In kết quả tìm được ra màn hình
                    System.out.println("(" + a + "," + b + "," + c + ")");
                    count++;

                    // Kiểm tra điều kiện dừng
                    if (count == N) {
                        break outerLoop; // Dừng khi đã liệt kê đủ N giá trị
                    }
                }
            }
        }

        // Đóng Scanner
        scanner.close();
    }
}
